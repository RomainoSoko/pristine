{
  "name": "pristine",
  "version": "1.0.0",
  "description": "JS Boilerplate for desktop, mobile and web apps using Webpack and Typescript.",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "w:c": "SET NODE_ENV=production && webpack --optimize-minimize",
    "w:w": "SET NODE_ENV=production && webpack --progress --colors --watch",
    "w:w:s": "SET NODE_ENV=browsersync && webpack --progress --colors --watch",
    "w:d": "SET NODE_ENV=development && webpack-dev-server",
    "i": "composer install && php bin/console doctrine:database:create && php bin/console doctrine:schema:create && php bin/console doctrine:fixtures:load && php bin/console ckeditor:install && php bin/console assets:install && npm install",
    "c": "composer install",
    "d:f:l": "php bin/console doctrine:fixture:load",
    "d:s:u": "php bin/console doctrine:schema:update --force",
    "d:d:d": "php bin/console doctrine:database:drop --force && php bin/console doctrine:database:create && php bin/console doctrine:schema:create && php bin/console doctrine:fixture:load",
    "cache:c": "php bin/console cache:clear --no-warmup && php bin/console cache:clear --env=prod --no-warmup",
    "g:m:d": "git checkout master && git merge dev && git push && git checkout dev",
    "g:c": "git config --global credential.helper 'cache --timeout 31536000'",
    "g:c:rm": "git credential-cache exit",
    "ncu": "ncu",
    "ncu:u": "ncu -u"
  },
